{
  "project": "Express",
  "outerHost": "express-website--adobe.hlx.live",
  "host": "www.adobe.com",
  "byocdn": true,
  "pushDownSelector": "#feds-header",
  "devMode": true,
  "plugins": [
    {
      "id": "metadata",
      "title": "Meta Data Inspector",
      "url": "https://main--express-website--adobe.hlx.page/tools/metadata/inspector.html",
      "environments": [ "edit" ],
      "excludePaths": [ "/**" ],
      "includePaths": [ "**metadata.xlsx**" ]
    },
    {
      "id": "templates",
      "title": "Templates",
      "url": "https://main--express-website--adobe.hlx.page/tools/templates/picker.html",
      "environments": [ "edit" ],
      "excludePaths": [ "/**" ],
      "includePaths": [ "**.docx**" ]
    },
    {
      "id": "experiments-local",
      "title": "Experiments (Local)",
      "url": "http://localhost:3030",
      "passConfig": true,
      "passReferrer": true,
      "isPalette": false,
      "environments": [ "edit", "admin", "edit", "dev", "preview", "live", "prod" ],
      "includePaths": [ "/**" ]
    },
    {
      "id": "experiments",
      "title": "Experiments",
      "url": "https://franklin-experimentation-genai-service.ethos09-prod-va7.ethos.adobe.net",
      "passConfig": true,
      "passReferrer": true,
      "isPalette": false,
      "environments": [ "edit", "admin", "edit", "dev", "preview", "live", "prod" ],
      "includePaths": [ "/**" ]
    },
    {
      "id": "experiments-azure",
      "title": "Experiments (Azure)",
      "url": "https://gentle-glacier-0d85da80f-preview.eastus2.3.azurestaticapps.net",
      "passConfig": true,
      "passReferrer": true,
      "isPalette": false,
      "paletteRect": "width: 60%; height: 60%; top: 20%; left: 20%;",
      "environments": [ "dev", "preview", "live", "prod" ],
      "includePaths": [ "/**" ]
    }
  ]
}
